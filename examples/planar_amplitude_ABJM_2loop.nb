Planar two-loop amplitude in ABJM

Tensor decomposition with FaRe

SetDirectory[NotebookDirectory[]]

<< "FeynCalc`" // Quiet

<< FaRe.m

(* 
p34=p3+p4;
p12=p1+p2
*)

Ilk = TRed[
   3, {l, k}, {{k, 1}, {k - p12, 1}, {l, 1}, {l + p34, 1}, {l + p4, 
     1}, {k - l, 1}, {k - p1, 1}}, {k, l}];

(* 
p3=-p1-p2-p4
*)

FIlk = Ilk /. {Pair[LorentzIndex[x_], Momentum["p34"]] :> 
      FourVector["p3", x] + FourVector["p4", x], 
     Pair[LorentzIndex[x_], Momentum["p12"]] :> 
      FourVector["p1", x] + FourVector["p2", x]} // FullSimplify;

FIlk1 = FIlk /. 
   Pair[LorentzIndex[x_], Momentum["p3"]] :> -FourVector["p1", x] - 
     FourVector["p2", x] - FourVector["p4", x];

sIlk = FullSimplify[Expand[FIlk1]];

(* g_munu coefficient *)

Cgmunu = Coefficient[sIlk, MetricTensor["m1", "m2"]]

A1 = FIREType[Cgmunu, 2, {0}, G] // ToExpression

(* p2_mu p2_nu coefficient *)

Cp2p2 = Coefficient[sIlk, 
  Pair[LorentzIndex["m1"], Momentum["p2"]] Pair[LorentzIndex["m2"], 
    Momentum["p2"]]]

B1 = FIREType[Cp2p2, 2, {0}, G] // ToExpression




Ik = TRed[3, {k}, {{k, 1}, {k - p12, 1}, {l + p34, 1}, {l + p4, 1}, {k - l, 
    1}, {k - p1, 1}}, {k, l}]

FIk = Ik /. {Pair[LorentzIndex[x_], Momentum["p34"]] :> 
      FourVector["p3", x] + FourVector["p4", x], 
     Pair[LorentzIndex[x_], Momentum["p12"]] :> 
      FourVector["p1", x] + FourVector["p2", x]} // FullSimplify;

FIk1 = FIk /. 
   Pair[LorentzIndex[x_], Momentum["p3"]] :> -FourVector["p1", x] - 
     FourVector["p2", x] - FourVector["p4", x];

sIk = FullSimplify[Expand[FIk1]]

Cp2k = Coefficient[sIk, Pair[LorentzIndex["m1"], Momentum["p2"]]] // Simplify

A2 = FIREType[Cp2k, 2, {3}, G] // ToExpression




Il = TRed[3, {l}, {{k - p12, 1}, {l, 1}, {l + p34, 1}, {l + p4, 1}, {k - l, 
    1}, {k - p1, 1}}, {k, l}]

FIl = Il /. {Pair[LorentzIndex[x_], Momentum["p34"]] :> 
      FourVector["p3", x] + FourVector["p4", x], 
     Pair[LorentzIndex[x_], Momentum["p12"]] :> 
      FourVector["p1", x] + FourVector["p2", x]} // FullSimplify;

FIl1 = FIl /. 
   Pair[LorentzIndex[x_], Momentum["p3"]] :> -FourVector["p1", x] - 
     FourVector["p2", x] - FourVector["p4", x];

sIl = FullSimplify[Expand[FIl1]]

Cp2l = Coefficient[sIl, Pair[LorentzIndex["m1"], Momentum["p2"]]] // Simplify

A3 = FIREType[Cp2l, 2, {1}, G] // ToExpression

